<%- partial(theme.header && theme.header.partial || '_ksio/partials/header') %>
<main class="Page-content">
  <%- partial('_ksio/slots/banner') %>
  <div class="container">
    <article class="Page-main Article">
      <header class="Article-header HomeHeader">
        <div>
          <h1 class="Article-title HomeHeader-title"><%= config.title %></h1>
          <p class="HomeHeader-slogan"><%= config.subtitle %></p>
        </div>
      </header>
      <div class="Article-content">
        <h2>实验动态</h2>
        <ul>
          <% const dailyData = site.data['knosys/dailies'] %>
          <% dailyData.sequence.slice().reverse().forEach(id => { %>
            <% const daily = dailyData.items[id] %>
            <li><time datetime="<%= date_xml(daily.date) %>" style="margin-right: 1em;"><span><%= date(daily.date, 'YYYY-MM-DD') %></span></time><a href="https://s.ourai.ws/dailies/<%= id %>/" target="_blank" rel="external" title="<%= daily.description %>"><%= daily.title %></a></li>
          <% }) %>
        </ul>
      </div>
    </article>
  </div>
</main>
<%- partial(theme.footer && theme.footer.partial || '_ksio/partials/footer') %>
